alias codehere="code -n ."

# Commonly used directories - assumes dev dir is in home
alias dev="cd $HOME/dev"
alias dbx="cd $HOME/Dropbox"
alias uni="cd $HOME/Dropbox/university"

alias mail="thunderbird"

# Quickly open and edit dotfiles from anywhere
alias editdot="code -n $HOME/dev/dotfiles"

alias tmux='tmux -2'
alias mux=tmuxinator

# Commonly used commands
alias g="git"

# Neovim with startup time logged
alias nvimst="nvim --startuptime startup.log"
